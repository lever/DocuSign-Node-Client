{
  "name": "docusign-node",
  "version": "3.1.0",
  "description": "A DocuSign API helper library",
  "main": "docusign.js",
  "scripts": {
    "lint": "node_modules/.bin/semistandard",
    "check-config": "./scripts/check-config.js",
    "test": "npm run check-config && node_modules/.bin/semistandard && node_modules/.bin/mocha"
  },
  "keywords": [
    "DocuSign",
    "electronic",
    "signature",
    "eSignature",
    "certificate",
    "DTM",
    "PDF"
  ],
  "license": {
    "type": "DocuSign",
    "url": "https://raw.githubusercontent.com/docusign/DocuSign-Node-Client/master/LICENSE"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "author": "Nikhil Mashettiwar <nikhil.mashettiwar@docusign.com>",
  "contributors": [
    "Chris Beiter <chris.beiter@docusign.com>",
    "CJ Winslow <whoaa512@gmail.com> (https://github.com/whoaa512)"
  ],
  "files": [
    "docusign.js",
    "dsUtils.js",
    "components",
    "scripts",
    "examples",
    "sample-config.json"
  ],
  "repository": "https://github.com/docusign/DocuSign-Node-Client",
  "dependencies": {
    "async": "0.9.0",
    "bluebird": "2.9.34",
    "lodash.assign": "3.2.0",
    "lodash.isempty": "3.0.4",
    "request": "2.22.0",
    "streamifier": "0.1.0",
    "temp": "0.8.1"
  },
  "devDependencies": {
    "chalk": "^1.1.0",
    "file-type": "2.5.0",
    "mocha": "2.2.4",
    "pre-commit": "1.0.10",
    "semistandard": "7.0.2"
  },
  "pre-commit": [
    "lint"
  ],
  "semistandard": {
    "ignore": [
      "ideal-*.js"
    ],
    "global": [
      "it",
      "describe",
      "before",
      "after"
    ]
  }
}
